{
  "name": "apirequests",
  "version": "0.4.1",
  "description": "Simple multiple configurable API requests.",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/ava/cli.js --verbose",
    "coverage": "nyc ava && nyc report --reporter=html",
    "lint": "node ./node_modules/eslint/bin/eslint.js lib/ index.js",
    "scan": "npm run audit && npm run security",
    "audit": "./node_modules/auditjs/audit.js --report --package package.json",
    "security": "./node_modules/nsp/bin/nsp check ."
  },
  "author": "DBProductions",
  "homepage": "https://github.com/DBProductions/apirequests",
  "repository": {
    "type": "git",
    "url": "https://github.com/DBProductions/apirequests.git"
  },
  "bugs": {
    "url": "https://github.com/DBProductions/apirequests/issues"
  },
  "keywords": [
    "api",
    "request",
    "test"
  ],
  "license": "MIT",
  "dependencies": {
    "cli-spinner": "0.2.8",
    "colors": "1.3.0",
    "joi": "13.4.0",
    "log-symbols": "2.2.0",
    "mongodb": "3.1.1",
    "request": "2.87.0",
    "sync-request": "6.0.0",
    "xml-writer": "1.7.0"
  },
  "devDependencies": {
    "auditjs": "2.4.4",
    "ava": "0.25.0",
    "eslint": "5.0.1",
    "eslint-config-standard": "12.0.0-alpha.0",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-node": "6.0.1",
    "eslint-plugin-promise": "3.8.0",
    "eslint-plugin-standard": "3.1.0",
    "nsp": "3.2.1",
    "nyc": "12.0.2"
  }
}
